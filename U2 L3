make: 
A = ["zebra", "kangaroo", "cat", "human", "aardvark"]

for i in range(len(A)):
      for j in range(i + 1, len(A)):
            if A[i] >= A[j]:
                  temp = A[i]
                  A[i] = A[j]
                  A[j] = temp

for i in range(len(A)):
      if i <= len(A) - 2:
            print(A[i], ", ", sep="", end="")
      else:
            print(A[i], sep="", end="")
